{
    "name": "yourname/laravel-forms",
    "description": "A flexible form builder and survey tool for Laravel applications",
    "type": "laravel-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "nwidart/laravel-modules": "^10.0"
    },
    "autoload": {
        "psr-4": {
            
            "Modules\\Forms\\": "app/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\Forms\\app\\Providers\\FormsServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
